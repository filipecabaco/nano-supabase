{
  "name": "nano-supabase",
  "version": "1.0.0",
  "description": "A lightweight Supabase-compatible client for PGlite - runs PostgreSQL entirely in the browser/edge",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm run build:bundle && pnpm run build:types",
    "build:bundle": "node scripts/build.js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:dev": "tsc",
    "clean": "rm -rf dist",
    "test:bun": "bun test tests/",
    "test:bun:auth": "bun test tests/auth.test.ts tests/auth-integration.test.ts",
    "test:bun:integration": "bun test tests/integration.test.ts tests/auth-integration.test.ts",
    "test:bun:parser": "bun test tests/postgrest-parser.test.ts",
    "test:bun:storage": "bun test tests/storage.test.ts tests/storage-flow.test.ts",
    "test:bun:pooler": "bun test tests/pooler.test.ts tests/queue.test.ts",
    "example:basic": "tsx examples/basic.ts",
    "example:server": "tsx examples/tcp-server.ts",
    "example:parser": "tsx examples/postgrest-parser.ts"
  },
  "keywords": [
    "pglite",
    "postgresql",
    "supabase",
    "postgrest",
    "edge",
    "webcontainer",
    "deno",
    "bun",
    "cloudflare-workers"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "@electric-sql/pglite": ">=0.3.0"
  },
  "peerDependenciesMeta": {
    "@electric-sql/pglite": {
      "optional": false
    }
  },
  "devDependencies": {
    "@electric-sql/pglite": "^0.3.15",
    "@supabase/supabase-js": "^2.93.3",
    "@types/node": "^25.1.0",
    "esbuild": "^0.24.0",
    "native_postgrest_parser": "github:filipecabaco/native_postgrest_parser",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "postgrest-parser": "^0.1.0"
  }
}
