{
  "name": "nano-supabase",
  "version": "0.1.0",
  "description": "A lightweight Supabase-compatible client for PGlite - runs PostgreSQL entirely in the browser/edge",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:bundle && npm run build:types",
    "build:bundle": "node scripts/build.js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:dev": "tsc",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test:bun": "bun test tests/",
    "test:bun:auth": "bun test tests/auth-integration.test.ts",
    "test:bun:data": "bun test tests/integration.test.ts tests/supabase-client.test.ts",
    "test:bun:storage": "bun test tests/storage.test.ts",
    "test:bun:fetch": "bun test tests/fetch-adapter.test.ts",
    "test:bun:apps": "bun test tests/applications.test.ts tests/full-user-flow.test.ts",
    "example:basic": "tsx examples/basic.ts",
    "example:server": "tsx examples/tcp-server.ts",
    "example:supabase": "bun run examples/supabase-client.ts",
    "example:parser": "bun run examples/postgrest-parser.ts",
    "example:prisma": "tsx examples/prisma-tcp.ts",
    "prisma:generate": "prisma generate --schema=examples/prisma/schema.prisma"
  },
  "keywords": [
    "pglite",
    "postgresql",
    "supabase",
    "postgrest",
    "edge",
    "webcontainer",
    "deno",
    "bun",
    "cloudflare-workers"
  ],
  "author": "filipecabaco",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/filipecabaco/nano-supabase.git"
  },
  "homepage": "https://github.com/filipecabaco/nano-supabase#readme",
  "bugs": {
    "url": "https://github.com/filipecabaco/nano-supabase/issues"
  },
  "peerDependencies": {
    "@electric-sql/pglite": ">=0.3.0"
  },
  "peerDependenciesMeta": {
    "@electric-sql/pglite": {
      "optional": false
    }
  },
  "devDependencies": {
    "@electric-sql/pglite": "^0.3.15",
    "@electric-sql/pglite-socket": "^0.0.21",
    "@prisma/client": "^6.19.2",
    "@supabase/supabase-js": "^2.93.3",
    "@types/node": "^25.1.0",
    "esbuild": "^0.27.3",
    "prisma": "^6.19.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "postgrest-parser": "^0.1.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "esbuild",
      "prisma"
    ]
  }
}
